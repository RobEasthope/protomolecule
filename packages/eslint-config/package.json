{
  "name": "@protomolecule/eslint-config",
  "version": "1.0.0",
  "description": "Shared ESLint configuration for Protomolecule projects",
  "main": "index.js",
  "files": [
    "index.js",
    "rules"
  ],
  "scripts": {
    "clean": "rm -rf node_modules",
    "format": "npx prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "tsc": "tsc --noEmit"
  },
  "devDependencies": {
    "@protomolecule/tsconfig": "workspace:*",
    "@types/eslint": "^9.6.1",
    "eslint": "^9.34.0",
    "eslint-config-canonical": "^45.0.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.42.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@types/node": "^20.12.12"
  }
}
