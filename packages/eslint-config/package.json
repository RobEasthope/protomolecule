{
  "name": "@robeasthope/eslint-config",
  "version": "6.2.1",
  "description": "Shared ESLint configuration with TypeScript and React support",
  "keywords": [
    "eslint",
    "eslint-config",
    "linting",
    "typescript",
    "astro",
    "code-quality"
  ],
  "homepage": "https://github.com/RobEasthope/protomolecule#readme",
  "bugs": {
    "url": "https://github.com/RobEasthope/protomolecule/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RobEasthope/protomolecule.git",
    "directory": "packages/eslint-config"
  },
  "license": "MIT",
  "author": {
    "name": "Rob Easthope",
    "url": "https://github.com/RobEasthope"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf node_modules dist",
    "format": "npx prettier --write .",
    "lint": "eslint '**/*.{js,jsx,ts,tsx,mjs}' --cache --cache-location ../../.turbo/.eslintcache-eslint-config",
    "lint:fix": "eslint '**/*.{js,jsx,ts,tsx,mjs}' --fix --cache --cache-location ../../.turbo/.eslintcache-eslint-config",
    "lint:fix-staged": "eslint '**/*.{js,jsx,ts,tsx,mjs}' --fix --max-warnings=Infinity --cache --cache-location ../../.turbo/.eslintcache-eslint-config || true",
    "lint:md": "markdownlint-cli2 '**/*.{md,mdx}' '!**/node_modules/**' '!**/dist/**' '!**/.turbo/**' '!**/.astro/**'",
    "lint:md:fix": "markdownlint-cli2 --fix '**/*.{md,mdx}' '!**/node_modules/**' '!**/dist/**' '!**/.turbo/**' '!**/.astro/**'",
    "prepublishOnly": "pnpm run build",
    "sort-pkg-json": "npx sort-package-json",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "astro-eslint-parser": "^1.0.0",
    "eslint-config-canonical": "^47.3.7",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsdoc": "^61.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-regexp": "^2.10.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^16.5.0",
    "typescript-eslint": "^8.50.0"
  },
  "devDependencies": {
    "@robeasthope/markdownlint-config": "workspace:*",
    "@robeasthope/tsconfig": "workspace:*",
    "@types/eslint": "^9.6.1",
    "@types/node": "^25.0.3",
    "eslint-import-resolver-typescript": "^4.4.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "eslint": "^8.57.0 || ^9.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
